generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model MediaWikiRecentChange {
  id        Int      @id @default(autoincrement())
  title     String?
  titleUrl  String?
  comment   String?
  timestamp DateTime @default(now())
  user      String?
  bot       Boolean?
  notifyUrl String?
  fullEdit  String?
  minor     Boolean?
  lengthOld Int?
  lengthNew Int?
  serverUrl String?
}
